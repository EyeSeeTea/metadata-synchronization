        "ts-mockito": "^2.6.1",
        "ts-node": "^10.8.2",
        "typescript": "4.9.5",
        "vite": "^4.0.0",
        "vite-plugin-node-polyfills": "^0.24.0",
        "vitest": "^3.2.4",
        "yargs": "15.4.1"
    },
    "resolutions": {
