        "jsdom": "^26.1.0",
        "miragejs": "0.1.42",
        "mocha": "9.1.3",
        "mochawesome": "6.3.1",
        "mochawesome-merge": "4.2.0",
        "prettier": "2.4.1",
        "prop-types": "15.7.2",
